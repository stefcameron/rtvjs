{
  "name": "rtvjs",
  "version": "0.0.1",
  "description": "Runtime Type Verification Isomorphic JavaScript Library",
  "main": "dist/rtv.umd.js",
  "module": "dist/rtv.esm.js",
  "jsnext:main": "dist/rtv.esm.js",
  "scripts": {
    "clean": "rm -rf ./build",
    "start": "node -r ./test/node.js",
    "build:docs": "jsdoc2md \"src/**/*.js\" > dist/rtv.js.md",
    "build:lib": "rollup -c",
    "build": "npm run clean && npm run build:lib && npm run build:docs",
    "lint": "eslint rollup.config.js \"src/**/*.js\"",
    "test:unit": "echo \"Error: no test specified\" && exit 1",
    "test": "npm run lint && npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/stefcameron/rtvjs.git"
  },
  "keywords": [
    "javascript",
    "test",
    "runtime",
    "verify",
    "verification",
    "assertion",
    "type",
    "integration",
    "isomorphic"
  ],
  "author": {
    "name": "Stefan Cameron",
    "url": "https://twitter.com/stefcameron"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/stefcameron/rtvjs/issues"
  },
  "homepage": "https://gitlab.com/stefcameron/rtvjs#README",
  "dependencies": {
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.2",
    "jsdoc-to-markdown": "^4.0.1",
    "rollup": "^0.56.5",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-uglify": "^3.0.0"
  }
}
